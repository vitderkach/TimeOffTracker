{
  "version": "1.0",
  "defaultProvider": "unpkg",
  "libraries": [
    {
      "provider": "filesystem",
      "library": "https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js",
      "destination": "wwwroot/lib/jquery/",
      "files": [
        "dist/jquery.js"
      ]
    },
    {
      "library": "popper.js@1.12.3",
      "destination": "wwwroot/lib/popper.js/",
      "files": [
        "dist/popper-utils.js",
        "dist/popper-utils.min.js",
        "dist/popper.js",
        "dist/popper.min.js"
      ]
    },
    {
      "library": "tooltip.js@1.3.3",
      "destination": "wwwroot/lib/tooltip.js/",
      "files": [
        "dist/tooltip.min.js",
        "dist/tooltip.js"
      ]
    },
    {
      "provider": "cdnjs",
      "library": "bootstrap-select@1.13.12",
      "destination": "wwwroot/lib/bootstrap-select/"
    },
    {
      "library": "bootstrap-datepicker@1.9.0",
      "destination": "wwwroot/lib/bootstrap-datepicker/"
    },
    {
      "library": "bootstrap@4.3.1",
      "destination": "wwwroot/lib/bootstrap/"
    },
    {
      "library": "bootstrap-icons@1.0.0-alpha3",
      "destination": "wwwroot/lib/bootstrap-icons/"
    }
  ]
}